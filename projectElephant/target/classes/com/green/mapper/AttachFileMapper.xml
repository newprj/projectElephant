<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	<mapper namespace="com.green.mapper.AttachFileMapper">
	
	<insert id="insert" >
		insert into attachFile values(#{rno}, #{fileName},#{uploadPath},#{uuid},#{fileSize})
	</insert>
	
	<select id="getList" resultType="com.green.vo.AttachFileDTO">
		select * from attachFile where rno=#{rno}
	</select> 
	
	<select id="read" resultType="com.green.vo.AttachFileDTO">
		select * from attachFile where uuid=#{uuid}
	</select> 
	
	<delete id="delete">
		delete from attachFile where rno = #{rno}
	</delete> 

</mapper>